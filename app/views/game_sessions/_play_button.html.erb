<% session_obj = local_assigns[:session] || @session %>
<% song_obj = local_assigns[:song] || @winning_song %>
<% can_start = local_assigns[:can_start] || false %>

<% if can_start %>
  <%= link_to "LET'S SING!",
      song_obj.youtube_url,
      target: "_blank",
      rel: "noopener noreferrer",
      class: "btn btn-outline-light goto_bt" %>
<% end %>
